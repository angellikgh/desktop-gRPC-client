{
  "name": "protogun",
  "productName": "protogun",
  "version": "1.0.0",
  "description": "My Electron application description",
  "main": ".webpack/main",
  "scripts": {
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish",
    "lint": "eslint --ext .ts,.tsx .",
    "lint:fix": "eslint --fix --ext .ts,.tsx .",
    "test": "jest --verbose=true",
    "test:cov": "jest --coverage --ci --reporters=default",
    "prepare": "husky install"
  },
  "keywords": [],
  "author": {
    "name": "Alexey Vasyukov",
    "email": "vasyukov.alexey.v@gmail.com"
  },
  "license": "MIT",
  "config": {
    "forge": {
      "packagerConfig": {},
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "protogun"
          }
        },
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "darwin"
          ]
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {}
        },
        {
          "name": "@electron-forge/maker-rpm",
          "config": {}
        }
      ],
      "plugins": [
        [
          "@electron-forge/plugin-webpack",
          {
            "mainConfig": "./webpack.main.config.js",
            "renderer": {
              "config": "./webpack.renderer.config.js",
              "entryPoints": [
                {
                  "html": "./src/app/index.html",
                  "js": "./src/app/renderer.tsx",
                  "name": "main_window",
                  "preload": {
                    "js": "./src/main/preload.ts"
                  }
                }
              ]
            }
          }
        ]
      ]
    }
  },
  "dependencies": {
    "@dnd-kit/core": "^5.0.3",
    "@dnd-kit/sortable": "^6.0.1",
    "@dnd-kit/utilities": "^3.1.0",
    "@fortawesome/fontawesome-svg-core": "^6.1.1",
    "@fortawesome/free-solid-svg-icons": "^6.1.1",
    "@fortawesome/react-fontawesome": "^0.1.18",
    "@grpc/grpc-js": "^1.6.7",
    "@grpc/proto-loader": "^0.6.12",
    "@nextui-org/react": "^1.0.0-beta.6",
    "ace-builds": "^1.4.14",
    "electron-squirrel-startup": "^1.0.0",
    "electron-store": "^8.0.1",
    "i18next": "^21.6.16",
    "nanoid": "^3.3.3",
    "rc-tabs": "^11.14.0",
    "react": "^18.1.0",
    "react-ace": "^10.1.0",
    "react-color": "^2.19.3",
    "react-dom": "^18.1.0",
    "react-hook-form": "^7.30.0",
    "react-i18next": "^11.16.7",
    "react-json-view": "^1.21.3",
    "react-select": "^5.3.2",
    "zustand": "^4.0.0-rc.1"
  },
  "devDependencies": {
    "@electron-forge/cli": "^6.0.0-beta.63",
    "@electron-forge/maker-deb": "^6.0.0-beta.63",
    "@electron-forge/maker-rpm": "^6.0.0-beta.63",
    "@electron-forge/maker-squirrel": "^6.0.0-beta.63",
    "@electron-forge/maker-zip": "^6.0.0-beta.63",
    "@electron-forge/plugin-webpack": "^6.0.0-beta.63",
    "@types/jest": "^27.5.0",
    "@types/react": "^18.0.8",
    "@types/react-color": "^3.0.6",
    "@types/react-dom": "^18.0.3",
    "@typescript-eslint/eslint-plugin": "^5.21.0",
    "@typescript-eslint/parser": "^5.21.0",
    "@vercel/webpack-asset-relocator-loader": "^1.7.2",
    "css-loader": "^6.7.1",
    "electron": "18.2.0",
    "eslint": "^8.14.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.29.4",
    "eslint-plugin-react-hooks": "^4.5.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "fork-ts-checker-webpack-plugin": "^7.2.9",
    "husky": "^7.0.4",
    "jest": "^28.1.0",
    "node-loader": "^2.0.0",
    "prettier": "^2.6.2",
    "style-loader": "^3.3.1",
    "ts-jest": "^28.0.1",
    "ts-loader": "^9.3.0",
    "typescript": "4.5.5"
  },
  "jest": {
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "coverageReporters": [
      "text"
    ],
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src/core",
    "testEnvironment": "node",
    "testRegex": ".spec.ts$",
    "transform": {
      ".+\\.(t|j)s$": "ts-jest"
    }
  }
}
