{
  "name": "protogun",
  "productName": "protogun",
  "version": "1.0.0-beta.1",
  "description": "Ultimate desktop gRPC client.",
  "main": ".webpack/main",
  "scripts": {
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish",
    "lint": "eslint --ext .ts,.tsx .",
    "lint:fix": "eslint --fix --ext .ts,.tsx .",
    "test": "jest --verbose=true",
    "test:cov": "jest --coverage --ci --reporters=default",
    "prepare": "husky install"
  },
  "keywords": [],
  "author": {
    "name": "Alexey Vasyukov",
    "email": "vasyukov.alexey.v@gmail.com"
  },
  "license": "MIT",
  "config": {
    "forge": {
      "packagerConfig": {},
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "protogun"
          }
        },
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "darwin"
          ]
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {}
        },
        {
          "name": "@electron-forge/maker-rpm",
          "config": {}
        }
      ],
      "plugins": [
        [
          "@electron-forge/plugin-webpack",
          {
            "mainConfig": "./webpack.main.config.js",
            "renderer": {
              "config": "./webpack.renderer.config.js",
              "entryPoints": [
                {
                  "html": "./src/app/index.html",
                  "js": "./src/app/renderer.tsx",
                  "name": "main_window",
                  "preload": {
                    "js": "./src/main/preload.ts"
                  }
                }
              ]
            }
          }
        ]
      ]
    }
  },
  "dependencies": {
    "@codemirror/commands": "6.0.1",
    "@codemirror/lang-json": "6.0.0",
    "@codemirror/language": "6.2.0",
    "@codemirror/lint": "6.0.0",
    "@codemirror/state": "6.1.0",
    "@codemirror/view": "6.0.3",
    "@dnd-kit/core": "6.0.5",
    "@dnd-kit/modifiers": "6.0.0",
    "@dnd-kit/sortable": "7.0.1",
    "@dnd-kit/utilities": "3.2.0",
    "@fortawesome/fontawesome-svg-core": "6.1.1",
    "@fortawesome/free-solid-svg-icons": "6.1.1",
    "@fortawesome/react-fontawesome": "0.2.0",
    "@grpc/grpc-js": "1.6.7",
    "@grpc/proto-loader": "0.6.13",
    "@lezer/highlight": "1.0.0",
    "@nextui-org/react": "1.0.0-beta.9",
    "@uiw/react-codemirror": "4.11.4",
    "chroma-js": "2.4.2",
    "electron-squirrel-startup": "1.0.0",
    "electron-store": "8.0.2",
    "i18next": "21.8.14",
    "immer": "9.0.15",
    "lodash": "4.17.21",
    "nanoid": "4.0.0",
    "re-resizable": "6.9.9",
    "react": "18.2.0",
    "react-color": "2.19.3",
    "react-dom": "18.2.0",
    "react-hook-form": "7.33.1",
    "react-i18next": "11.18.1",
    "react-json-view": "1.21.3",
    "react-select": "5.4.0",
    "react-use": "17.4.0",
    "zustand": "4.0.0-rc.1"
  },
  "devDependencies": {
    "@electron-forge/cli": "6.0.0-beta.64",
    "@electron-forge/maker-deb": "6.0.0-beta.64",
    "@electron-forge/maker-rpm": "6.0.0-beta.64",
    "@electron-forge/maker-squirrel": "6.0.0-beta.64",
    "@electron-forge/maker-zip": "6.0.0-beta.64",
    "@electron-forge/plugin-webpack": "6.0.0-beta.64",
    "@types/chroma-js": "2.1.4",
    "@types/jest": "28.1.5",
    "@types/lodash": "4.14.182",
    "@types/react": "18.0.15",
    "@types/react-color": "3.0.6",
    "@types/react-dom": "18.0.6",
    "@typescript-eslint/eslint-plugin": "5.30.6",
    "@typescript-eslint/parser": "5.30.6",
    "@vercel/webpack-asset-relocator-loader": "1.7.3",
    "css-loader": "6.7.1",
    "electron": "19.0.8",
    "eslint": "8.19.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-airbnb-typescript": "17.0.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-jsx-a11y": "6.6.0",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-react": "7.30.1",
    "eslint-plugin-react-hooks": "4.6.0",
    "eslint-plugin-simple-import-sort": "7.0.0",
    "fork-ts-checker-webpack-plugin": "7.2.12",
    "husky": "8.0.1",
    "jest": "28.1.3",
    "node-loader": "2.0.0",
    "prettier": "2.7.1",
    "style-loader": "3.3.1",
    "ts-jest": "28.0.5",
    "ts-loader": "9.3.1",
    "typescript": "4.7.4"
  },
  "jest": {
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "coverageReporters": [
      "text"
    ],
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src/core",
    "testEnvironment": "node",
    "testRegex": ".spec.ts$",
    "transform": {
      ".+\\.(t|j)s$": "ts-jest"
    }
  }
}
